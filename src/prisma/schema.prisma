generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id @default(autoincrement())
  username String  @unique
  password String
  isAdmin  Boolean @default(false)
}

model Books {
  id Int @id @default(autoincrement())
  title String
  author String
  available Boolean @default(true)
}